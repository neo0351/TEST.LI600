LI600_read_flash <- function(file_loc){
# read top data (time, data, li600 info, fm, fv, etc)
system.time(flash_info <- read.csv(file_loc, nrows=2)[-1,])
# read second header
system.time(header2 <- read.csv(file_loc, skip=4, nrows=1, header=F))
# read flash data
system.time(flash_data <- read.csv(file_loc, skip=6, header=F))
# add header to flash data
names(flash_data) <- header2
# combine flash info and data
out_df <- data.frame(flash_data, flash_info, row.names=NULL)
return(out_df)
}
