names(flash_data) <- flash_df[3, 1:4]
out_df <- type.convert(data.frame(flash_data, flash_df[2,]), as.is=T)
out_df$date <- as.POSIXct(paste(out_df$date, out_df$time))
out_df <- out_df[,!names(out_df) %in% "time"]
return(out_df)
}
